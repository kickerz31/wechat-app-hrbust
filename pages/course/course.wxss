page {
  background-color: #fff;
  color: #000;
}


.timetableWrapper {
  margin-left: 5rem;
  font-size: 16px;
  display: flex;
  position: relative;
  justify-content: space-between;
}
.timetableContent {
  border: 1px solid #eeeeee;
  flex: 1;
  line-height: 24px;
  margin-left: -1px;
  margin-bottom: -1px;
  /*background-color: #a1887f*/
}
.timetableWrapper:first-child .timetableContent {
  padding: 0;
  text-align: center;
}
.wrap {
  width: 1000px;
}
.topWrap {
  width: 20rem;
  /*overflow: hidden;*/
}

.time {
  position: absolute;
  left: -5rem;
  border: 1px solid #eeeeee;

}

.timetableWrapper:first-child .timetableContent{
  /*background-color: #ff8a65;*/
  position: relative;
  top: 0;
}

.courseItem {
  border-top: 10px solid #fff;
  padding: 10px;
}

.todayStyle {
  border-top: 10px solid #2196f3;
  margin: 0 -1px;
}
